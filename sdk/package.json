{
  "name": "rgb-ldk-node-sdk",
  "version": "0.1.0",
  "description": "TypeScript client for the rgb-ldk-node HTTP API",
  "license": "MIT OR Apache-2.0",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
